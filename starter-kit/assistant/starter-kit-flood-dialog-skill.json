      "output": {
        "generic": [
          {
            "values": [
              {
                "text": "Flooding is a temporary overflow of water onto land that is normally dry. It results from rain, snow, coastal storms, storm surges, and overflows of dams and other water systems. Floods can cause outages, disrupt transportation, damage buildings, and create landslides."
              }
            ],
            "response_type": "text",
            "selection_policy": "sequential"
          }
        ]
      },
      "conditions": "#Flood_About",
      "dialog_node": "node_5_1584231112927",
